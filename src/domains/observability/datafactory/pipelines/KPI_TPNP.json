                   "type":"String",
                   "physicalType":"string"
                }
             },
             {
                "source":{
                   "name":"iuv",
                   "type":"String",
                   "physicalType":"string"
                },
                "sink":{
                   "name":"IUV",
                   "type":"String",
                   "physicalType":"string"
                }
             },
             {
                "source":{
                   "name":"ccp",
                   "type":"String",
                   "physicalType":"string"
                },
                "sink":{
                   "name":"TOKEN",
                   "type":"String",
                   "physicalType":"string"
                }
             },
             {
