                },
                "sink":{
                   "name":"ID_BROKER_PSP",
                   "type":"String",
                   "physicalType":"string"
                }
             },
             {
                "source":{
                   "name":"psp",
                   "type":"String",
                   "physicalType":"string"
                },
                "sink":{
                   "name":"PSP",
                   "type":"String",
                   "physicalType":"string"
                }
             },
             {
                "source":{
                   "name":"channel",
                   "type":"String",
                   "physicalType":"string"
                },
                "sink":{
                   "name":"CANALE",
                   "type":"String",
                   "physicalType":"string"
                }
             },
             {
                "source":{
                   "name":"outcome",
                   "type":"String",
                   "physicalType":"string"
                },
                "sink":{
                   "name":"OUTCOME",
                   "type":"String",
                   "physicalType":"string"
                }
             },
             {
                "source":{
                   "name":"diff_time_in_ms",
                   "type":"Int64",
                   "physicalType":"long"
                },
                "sink":{
