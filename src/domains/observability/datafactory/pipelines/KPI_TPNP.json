                   "type":"String"
                },
                "sink":{
                   "name":"pipelineRunID",
                   "type":"String",
                   "physicalType":"string"
                }
             }
          ],
          "typeConversion":true,
          "typeConversionSettings":{
             "allowDataTruncation":true,
             "treatBooleanAsNumber":false
          }
       }
    },
    "inputs":[
