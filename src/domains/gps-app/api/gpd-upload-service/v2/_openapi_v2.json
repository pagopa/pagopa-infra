  "paths": {
    "/brokers/{broker-code}/organizations/{organization-fiscal-code}/debtpositions/file/{file-id}/report": {
      "get": {
        "tags": [
          "Massive operation observability APIs - v2"
        ],
        "summary": "Returns the debt positions upload report.",
        "operationId": "get-debt-positions-upload-report",
        "parameters": [
          {
            "name": "broker-code",
            "in": "path",
            "description": "The broker code",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "minLength": 1,
              "type": "string"
            }
          },
          {
            "name": "organization-fiscal-code",
            "in": "path",
            "description": "The organization fiscal code",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "minLength": 1,
              "type": "string"
            }
          },
          {
            "name": "file-id",
            "in": "path",
            "description": "The unique identifier for file upload",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "minLength": 1,
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Upload report found.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UploadReportDTO"
                }
              }
            }
          },
          "400": {
            "description": "Malformed request.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemJson"
                }
              }
            }
          },
          "401": {
            "description": "Wrong or missing function key.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemJson"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "404": {
            "description": "Upload report not found.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemJson"
                }
              }
            }
          },
          "429": {
            "description": "Too many requests.",
            "content": {
              "text/json": {}
            }
          },
          "500": {
            "description": "Service unavailable.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemJson"
                }
              }
            }
          }
        },
        "security": [
          {
            "ApiKey": []
          }
        ]
      }
    },
