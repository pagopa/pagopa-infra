    "/brokers/{broker-code}/organizations/{organization-fiscal-code}/debtpositions/files": {
      "get": {
        "tags": [
          "Massive operation observability APIs - v2"
        ],
        "summary": "Returns the list of fileIds for a broker/organization in the given date range (max 7 days).",
        "operationId": "get-debt-positions-fileids",
        "parameters": [
          {
            "name": "broker-code",
            "in": "path",
            "description": "The broker code",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "minLength": 1,
              "type": "string"
            }
          },
          {
            "name": "organization-fiscal-code",
            "in": "path",
            "description": "The organization fiscal code",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "minLength": 1,
              "type": "string"
            }
          },
          {
            "name": "from",
            "in": "query",
            "description": "Start date (YYYY-MM-DD), Europe/Rome",
            "required": false,
            "style": "form",
            "explode": false,
            "schema": {
              "type": "string"
            },
            "example": "2025-09-01"
          },
          {
            "name": "to",
            "in": "query",
            "description": "End date (YYYY-MM-DD), Europe/Rome",
            "required": false,
            "style": "form",
            "explode": false,
            "schema": {
              "type": "string"
            },
            "example": "2025-09-06"
          },
          {
            "name": "size",
            "in": "query",
            "description": "Max items per page (default 100, min 100, max 500)",
            "required": false,
            "style": "form",
            "explode": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 100
            }
          },
          {
            "name": "x-continuation-token",
            "in": "header",
            "description": "Continuation token (opaque). Pass it back to get the next page.",
            "required": false,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string",
              "nullable": true
            }
          }
        ],
        "responses": {
          "200": {
            "description": "FileIds retrieved.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FileIdListResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemJson"
                }
              }
            }
          },
          "401": {
            "description": "Wrong or missing function key.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemJson"
                }
              }
            }
          },
          "500": {
            "description": "Service unavailable.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemJson"
                }
              }
            }
          }
        },
        "security": [
          {
            "ApiKey": []
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "FileIdListResponse": {
        "type": "object",
        "properties": {
          "fileIds": {
            "type": "array",
            "description": "List of file identifiers",
            "items": {
              "type": "string"
            }
          },
          "size": {
            "type": "integer",
            "description": "Number of items in this page",
            "format": "int32"
          },
          "hasMore": {
            "type": "boolean",
            "description": "True if another page is available (x-continuation-token present)"
          }
        }
      },
      "ProblemJson": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "description": "A short, summary of the problem type. Written in english and readable for engineers (usually not suited for non technical stakeholders and not localized); example: Service Unavailable"
          },
          "status": {
            "type": "integer",
            "description": "The HTTP status code generated by the origin server for this occurrence of the problem.",
            "format": "int32",
            "example": 200
          },
          "detail": {
            "type": "string",
            "description": "A human readable explanation specific to this occurrence of the problem.",
            "example": "There was an error processing the request"
          }
        }
      },
      "ResponseEntryDTO": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "integer",
            "format": "int32",
            "example": 400
          },
          "statusMessage": {
            "type": "string",
            "example": "Bad request caused by invalid email address"
          },
          "iupds": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "UploadReportDTO": {
        "type": "object",
        "properties": {
          "fileId": {
            "type": "string"
          },
          "processedItem": {
            "type": "integer",
            "format": "int32"
          },
          "submittedItem": {
            "type": "integer",
            "format": "int32"
          },
          "responses": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ResponseEntryDTO"
            }
          },
          "startTime": {
            "type": "string",
            "format": "date-time",
            "example": "2024-10-08T14:55:16.302Z"
          },
          "endTime": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "UploadStatus": {
        "type": "object",
        "properties": {
          "uploadID": {
            "type": "string"
          },
          "processedItem": {
            "type": "integer",
            "format": "int32"
          },
          "submittedItem": {
            "type": "integer",
            "format": "int32"
          },
          "startTime": {
            "type": "string",
            "format": "date-time",
            "example": "2024-10-08T14:55:16.302Z"
          }
        }
      }
    },
    "securitySchemes": {
      "ApiKey": {
        "type": "apiKey",
        "name": "ApiKey",
        "in": "header"
      }
    }
  }
